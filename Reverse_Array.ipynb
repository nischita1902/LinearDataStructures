      "source": [
        "def reverse_array(arr):\n",
        "    left , right = 0 , len(arr) -1\n",
        "\n",
        "    while left < right:\n",
        "      arr[left], arr[right] = arr[right] , arr[left]\n",
        "      left += 1\n",
        "      right -= 1\n",
        "\n",
        "arr = [1,2,3,4,5,6]\n",
        "print(\"Orignal array is: \",arr)\n",
        "\n",
        "reverse_array(arr)\n",
        "print(\"Reversed array is: \",arr)"
      ],
